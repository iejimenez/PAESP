
@{
    ViewData["Title"] = "Index";
}


@section scripts{
    <script src="~/Scripts/CalendarioAcademico.js" type="module"></script>
}


<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <div class="card-title">
                    <h1>Calendarios Academicos</h1>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-lg-10">
                        <table class="table" id="datatable_calendario">
                            <thead>
                                <tr>
                                    <th>Descripción</th>
                                    <th>Periodo</th>
                                    <th>Fecha inicio</th>
                                    <th>Fecha final</th>
                                    <th>Semestre</th>
                                    <th>Año academico</th>
                                    <th>Opciones</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

