



@{
    ViewData["Title"] = "index";
}

@section scripts{

    <script src="~/Scripts/Preinscripcion.js" type="module"></script>

}


<div class="row">
    <div class="col-lg-12">

        <div class="card border-0 shadow-none">

            <div class="card-header">
                <h2> Liquidador de conceptos </h2>
            </div>

            <div class="card-body p-1">

                <form id="form_preinscripcion" name="form-preinscripcion">
                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Tipo de identificacion</label>
                        <div class="col-sm-4">
                            <select class="form-control" id="txtTipoIdentificacion" name="txtTipoIdentificacion"></select>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Identificación</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtNroDocumento" name="txtNroDocumento" />
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Nombres</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtNombres" name="txtNombres" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Apellidos</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtApellidos" name="txtApellidos" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Email</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtEmail" name="txtEmail" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Telefono</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtTelefono" name="txtTelefono" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Ciudad</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtCiudad" name="txtCiudad" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="" class="col-sm-2 control-label">Concepto</label>
                        <div class="col-sm-4">
                            <select class="form-control" id="txtConcepto" name="txtConcepto"></select>
                        </div>
                    </div>
                   

                </form>

                <div class="row">
                    <div class="col-xl-12 text-right">
                        <button type="button" id="btnGenerarRecibo" class="btn btn-primary">Generar Recibo</button>
                    </div>
                </div>
            </div>
        </div>

    </div>

</div>

